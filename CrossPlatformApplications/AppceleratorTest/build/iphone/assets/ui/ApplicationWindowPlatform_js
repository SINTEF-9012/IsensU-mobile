function ApplicationWindowPlatform(t,e,i,n){if(i&&e.addEventListener("load",function(){t.title=e.evalJS("document.title")}),n){var a=require("/ui/Drawer"),o=new a(t),r=Ti.UI.createButton({backgroundImage:"/images/LeftArrow.png",width:48,height:48});r.addEventListener("click",function(){e.goBack()}),o.buttonBar.add(r);var d=Ti.UI.createButton({backgroundImage:"/images/RightArrow.png",width:48,height:48});d.addEventListener("click",function(){e.goForward()}),o.buttonBar.add(d),t.add(o.view)}}module.exports=ApplicationWindowPlatform;