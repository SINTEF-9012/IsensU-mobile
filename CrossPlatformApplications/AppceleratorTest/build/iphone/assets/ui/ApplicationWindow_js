function ApplicationWindow(){var e=0,t=!1,i=!1,r=!1,a="#F5FFFA",o=!1;Ti.Platform.osname;var n=Ti.UI.createWindow({backgroundColor:"#F5FFFA",navBarHidden:!r,modal:!0,exitOnClose:!0});if(t){var d=Ti.UI.createView({left:e,top:e,right:e,bottom:e,borderRadius:5,borderWidth:1,borderColor:a,backgroundColor:a,opacity:i?0:.75});n.add(d),i&&setTimeout(function(){d.animate(Ti.UI.createAnimation({opacity:.75,duration:2e3}))},1),e=2*e}var p=Ti.UI.createWebView({left:e,top:e,right:e,bottom:e,backgroundColor:"#F5FFFA",opacity:i?0:1,enableZoomControls:!1,url:"/HTML/index.html"});n.add(p),i&&setTimeout(function(){p.animate(Ti.UI.createAnimation({opacity:1,duration:2e3}))},1);var l;return l="mobileweb"==Ti.Platform.osname?require("mobileweb/ui/ApplicationWindowPlatform"):require("ui/ApplicationWindowPlatform"),l(n,p,r,o),n}module.exports=ApplicationWindow;